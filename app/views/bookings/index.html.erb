<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <h1>Return a book</h1>
      <div class="form-container">
        <%= simple_form_for @booking do |f| %>
          <%= f.input :book_id, collection: @books %>
          <%= f.submit class: "btn btn-warning"%>
        <% end %>
      </div>
    </div>
  </div>
</div>
